@using Plumberz.Core.Models;
@model List<Technician>
<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Name</th>
            <th scope="col">Surname</th>
            <th scope="col">Position</th>
        </tr>
    </thead>
        <tbody>
    @foreach (var item in Model)
    {
            <tr>
                <th scope="row">1</th>
                <td>@item.Name</td>
                <td>@item.Surname</td>
                <td>@item.Position</td>
            </tr>
    } 
        </tbody>
</table>